{
  "version": 2,
  "routes": [
    { 
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/graffiti/(.*)",
      "dest": "/graffiti/$1"
    },
    {
      "src": "/am.png",
      "dest": "/am.png"
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      },
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build"
} 